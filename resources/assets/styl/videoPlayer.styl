/* VideoPlayer.styl */
.video-player
  max-width 900px
  text-align left
  margin 200px auto
@media screen and (max-width 700px)
  .video-player
    max-width 640px
    text-align left
    margin 200px auto
@media screen and (max-width 400px)
  .video-player
    max-width 380px
    text-align left
    margin 200px auto